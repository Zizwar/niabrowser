{
  "expo": {
    "name": "نيةبراوزر - البراوزر الذكي",
    "slug": "niabrowser-smart-browser",
    "version": "2.0.0",
    "orientation": "default",
    "description": "Advanced developer browser with DevTools, JavaScript injection, network monitoring, and Greasemonkey support",
    "privacy": "public",
    "primaryColor": "#000000",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zizwar.niabrowser",
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "zizwar",
    "plugins": [
      "expo-web-browser",
      "expo-media-library"
    ],
    "keywords": [
      "developer",
      "browser",
      "devtools",
      "javascript",
      "webview"
    ],
    "githubUrl": "https://github.com/zizwar/niabrowser",
    "extra": {
      "eas": {
        "projectId": "555e1621-5260-4544-b3aa-5097690cbaee"
      }
    }
  }
}
