{
  "expo": {
    "name": "NIABrowser",
    "slug": "niabrowser-smart-devtools",
    "version": "1.1.1",
    "orientation": "default",
    "description": "AI-powered developer browser: Generate scripts, monitor networks, execute code",
    "primaryColor": "#000000",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E8E8EC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibzcode.niabrowser"
    },
    "scheme": "niabrowser",
    "android": {
      "package": "com.vibzcode.niabrowser",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5F5F0"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": false,
          "data": [
            {
              "scheme": "http"
            },
            {
              "scheme": "https"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "zizwar",
    "plugins": [
      "expo-web-browser",
      "expo-secure-store",
      "expo-font",
      "expo-sharing"
    ],
    "githubUrl": "https://github.com/zizwar/niabrowser",
    "extra": {
      "eas": {
        "projectId": "f3de0a65-ce2b-4752-9b82-4ed5e4e4dabe"
      }
    }
  }
}
