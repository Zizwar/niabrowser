{
  "expo": {
    "name": "WebZview Developer Browser",
    "slug": "webzview-developer-browser",
    "version": "2.0.0",
    "orientation": "default",
    "description": "Advanced developer browser with DevTools, JavaScript injection, network monitoring, and Greasemonkey support",
    "privacy": "public",
    "primaryColor": "#000000",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zizwar.webzview",
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "34f041e6-855a-481c-a530-b068d0e3a889"
      }
    },
    "owner": "zizwar",
    "plugins": [
      "expo-web-browser",
      "expo-media-library"
    ],
    "keywords": ["developer", "browser", "devtools", "javascript", "webview"],
    "githubUrl": "https://github.com/zizwar/webzview"
  }
}
